let id = coh [(x) : x -> x]

normalise (a:*)(b: a -> a) | id(b)

let id2 = coh [(x(f)y): f -> f]

normalise (a:*) (b: a-> a) | id2(b)

let disc = coh[(x(f)y): x -> y]

normalise (a: *) (b: a -> a) | disc(b)

let comp = coh[(x(f)y(g)z): x -> z]

let endo = coh[(x(f)y(g)z): comp(f,g) -> comp(f,g)]

normalise (a: *) (b: a -> a) | endo(b,b)

normalise (a: *) (b: a -> a) | comp(id(a),b)
